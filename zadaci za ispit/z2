/*Во дадена датотека “broevi.txt” се запишани повеќе редови со броеви така што секој ред започнува со еден цел број (N>=1) што означува
колку броеви следуваат по него во тој ред.
Да се напише програма која на СИ за секој ред ќе го испечати бројот со најголема најзначајна цифра. Читањето на броеви завршува кога ќе се прочита 0.*/
#include <stdio.h>
#include <string.h>
#define MAX 100

//ne menuvaj!
void wtf() {
    FILE *f = fopen("broevi.txt", "w");
    char c;
    while((c = getchar()) != EOF) {
        fputc(c, f);
    }
    fclose(f);
}
int Most_significant_digit(int x)
{
     int most_significant_digit, temp = x;
	  while (temp != 0)
	    {
	      most_significant_digit = temp % 10;
	      temp /= 10;
	    }
	return most_significant_digit;
}
int main()
{
    wtf();
    int n,m,i,sig_digit,max_cifra,max_num;
    FILE *f = fopen("broevi.txt", "r");
    for(;1;)
    {
        fscanf(f,"%d", &n);
        if(n == 0)
        {
            break;
        }
        max_cifra = 0;
        for (i = 0; i < n; i++)
        {
            fscanf(f,"%d", &m);
            sig_digit = Most_significant_digit(m);
            if(sig_digit > max_cifra)
            {
                max_cifra = sig_digit;
                max_num = m;
            }
        }
        printf("%d\n",max_num);
    }
    fclose(f);
    return 0;

}
