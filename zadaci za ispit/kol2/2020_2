#include <stdio.h>
#include <string.h>
#include <math.h>
int main() {
    int matrix[100][100],n;
    scanf("%d",&n);
    int N = 1,i,j;
    int dimenzija = sqrt(n);
    for(i = 0; i < dimenzija+1; i++){
        for(j = 0; j < dimenzija+1; j++)
    {
        if(n >= N){
        matrix[j][i] = N;
        N++;}
        else
        {
            matrix[j][i] = 0;
        }
    }}
    for (i = 0; i < dimenzija+1; i++)
    {
        for(j = 0; j < dimenzija+1; j++)
        {
            printf("%d ",matrix[i][j]);
        }
        printf("\n");
    }
    FILE *f = fopen("input.txt","w");
    for(i = 0; i<dimenzija+1; i++)
    {
        for(j = 0; j<dimenzija+1; j++)
        {
            fprintf(f,"%d ",matrix[i][j]);
        }
        fprintf(f,"\n");
    }
    fclose(f);
    return 0;
}
